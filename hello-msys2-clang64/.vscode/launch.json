{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "msys2/Clang++ Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [
        "/C",
        "&",
        "pause"
      ],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "program": "${fileDirname}/${fileBasenameNoExtension}.exe",
      "internalConsoleOptions": "openOnFirstSessionStart",
      "MIMode": "lldb",
      "miDebuggerPath": "E:/Cplus/msys64/clang64/bin/lldb-mi.exe",
      "externalConsole": false,
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "postDebugTask": "",
      "preLaunchTask": "msys2 clang++ build"
    },
    {
      "name": "msys2/Clang Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [
        "/C",
        "&",
        "pause"
      ],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "program": "${fileDirname}/${fileBasenameNoExtension}.exe",
      "internalConsoleOptions": "openOnFirstSessionStart",
      "MIMode": "lldb",
      "miDebuggerPath": "E:/Cplus/msys64/clang64/bin/lldb-mi.exe",
      "externalConsole": false,
      "avoidWindowsConsoleRedirection": true,
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "msys2 clang build"
    }
  ]
}