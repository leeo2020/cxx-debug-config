{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C++ Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [
        "/C",
        "&",
        "pause"
      ],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "program": "${fileDirname}/${fileBasenameNoExtension}.exe",
      "internalConsoleOptions": "openOnSessionStart",
      "MIMode": "gdb",
      "miDebuggerPath": "E:/Cplus/cygwin/bin/gdb.exe",
      "externalConsole": false,
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "postDebugTask": "",
      "preLaunchTask": "cygwin g++ build"
    },
    {
      "name": "Clang Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [
        "/C",
        "&",
        "pause"
      ],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "program": "${fileDirname}/${fileBasenameNoExtension}.exe",
      "internalConsoleOptions": "openOnSessionStart",
      "MIMode": "gdb",
      "miDebuggerPath": "E:/Cplus/cygwin/bin/gdb.exe",
      "externalConsole": false,
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "cygwin gcc build"
    }
  ]
}